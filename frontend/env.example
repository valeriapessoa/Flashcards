# Configuração da API
NEXT_PUBLIC_API_URL=http://localhost:5000

# Configuração do NextAuth
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=sua_chave_secreta_aqui

# Configuração do Google OAuth
NEXT_PUBLIC_GOOGLE_CLIENT_ID=seu_google_client_id
GOOGLE_CLIENT_SECRET=sua_google_client_secret

# Configuração do Facebook OAuth
NEXT_PUBLIC_FACEBOOK_CLIENT_ID=seu_facebook_client_id
FACEBOOK_CLIENT_SECRET=sua_facebook_client_secret

# Configuração do Cloudinary
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=seu_cloud_name
NEXT_PUBLIC_CLOUDINARY_API_KEY=sua_api_key
CLOUDINARY_API_SECRET=sua_api_secret

# Modo de depuração (apenas desenvolvimento)
NEXT_PUBLIC_DEBUG=false
